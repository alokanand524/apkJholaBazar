{
  "expo": {
    "name": "Jhola Bazar",
    "slug": "jhola-bazar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/jhola-bazar1.png",
    "scheme": "jholabazar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/jhola-bazar1.png",
        "backgroundImage": "./assets/images/jhola-bazar.png",
        "monochromeImage": "./assets/images/jhola-bazar.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jholabazar.app",
      "config": {
        "googleMaps": {
          "apiKey": "$EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/jhola-bazar1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to show nearby stores and delivery options."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "googleMapsApiKey": "$EXPO_PUBLIC_GOOGLE_MAPS_API_KEY",
      "eas": {
        "projectId": "c77f147a-c2bd-4d7e-b3bc-91f3eaa0ae24"
      }
    },
    "owner": "vikas10237"
  }
}
