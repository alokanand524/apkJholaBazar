{
  "expo": {
    "name": "Jhola Bazar",
    "slug": "jhola-bazar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon_logo.png",
    "scheme": "jholabazar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jholabazar.app",
      "googleServicesFile": "$GOOGLE_SERVICES_JSON",
      "config": {
        "googleMaps": {
          "apiKey": "$EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.POST_NOTIFICATIONS",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "allowBackup": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app_icon_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app_icon_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to show nearby stores and delivery options."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app_notification.png",
          "color": "#fffae6",
          "defaultChannel": "default"
        }
      ],
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "googleMapsApiKey": "$EXPO_PUBLIC_GOOGLE_MAPS_API_KEY",
      "eas": {
        "projectId": "c77f147a-c2bd-4d7e-b3bc-91f3eaa0ae24"
      }
    },
    "owner": "vikas10237"
  }
}
